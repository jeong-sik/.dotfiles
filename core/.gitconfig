[core]
	editor = nvim
	excludesfile = ~/.gitignore-global
	fsmonitor = true
	untrackedCache = true
[merge]
	conflictstyle = diff3
[diff]
	tool = vimdiff
[feature]
	manyFiles = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
